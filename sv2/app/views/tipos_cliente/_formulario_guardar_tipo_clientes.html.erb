<%= form_for @tipoCliente do |f| %>
    <i>Campos obligatorios (*)</i>
    <p></p>
    <div class="field">
      <%= f.label :tipo_cliente_descr, "Tipo de Cliente (*)" %><br />
      <%= f.text_field :tipo_cliente_descr, autofocus: true %>
    </div>

    <p></p>
    <div class="actions">
      <%= f.submit "Registrar" %>
    </div>
<% end %>